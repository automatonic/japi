{
  "version": "0.1.1",
  "title": "JApi",
  "description": "JSON.NET extensions for supporting JSON API (http://jsonapi.org/)",
  "language": "en-US",
  "authors": [ "Elliott B. Edwards (automatonic)" ],
  "packOptions": {
    "projectUrl": "https://www.github.com/automatonic/japi",
    "tags": [ "json", "json-api", "rest" ],
    "iconUrl": "http://www.gravatar.com/avatar/772743edd14cb0bd749c2a08bccad4c5.png?s=256",
    "licenseUrl": "https://raw.github.com/automatonic/japi/master/LICENSE.md",
    "repository": {
      "type": "git",
      "url": "git://github.com/automatonic/japi"
    }
  },
  "buildOptions": {
    "outputName": "JApi",
    "debugType": "portable"
  },
  "dependencies": {
    "Newtonsoft.Json": "9.0.1"
  },
  "frameworks": {
    "netstandard1.0": {
      "dependencies": {
        "NETStandard.Library": "1.0.0"
      }
    }
  },
  "tooling": {
    "defaultNamespace": "JApi"
  }
}
